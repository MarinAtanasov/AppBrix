<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <Description>Sample web application using an AppBrix app.</Description>
    <Copyright>Copyright 2015-2019</Copyright>
    <AssemblyTitle>AppBrix.WebApp</AssemblyTitle>
    <Version>0.13.0</Version>
    <Authors>Marin Atanasov</Authors>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <AspNetCoreHostingModel>InProcess</AspNetCoreHostingModel>
    <GenerateDocumentationFile>false</GenerateDocumentationFile>
    <AssemblyName>AppBrix.WebApp</AssemblyName>
